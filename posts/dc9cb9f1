    if (typeof Artalk === 'object') initArtalk(el, pageKey)
    else {
      await btf.getCSS('/yww/styles/Artalk.css')
      await btf.getScript('/yww/scripts/Artalk.js')
      initArtalk(el, pageKey)
    }
