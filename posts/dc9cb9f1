      el: el.querySelector('#artalk-wrap'),
      server: 'https://comment.yww52.com',
      site: 'yww的博客',
      darkMode: document.documentElement.getAttribute('data-theme') === 'dark',
      ...option,
      pageKey: isShuoshuo ? pageKey : (option && option.pageKey) || pageKey
